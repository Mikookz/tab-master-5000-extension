{
  "name": "tab-master-5000",
  "version": "2.8.0",
  "description": "Tab Master 5000",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaszhix/tab-master-5000-chrome-extension"
  },
  "scripts": {
    "dev-chrome": "cross-env DEV_ENV=chrome NODE_ENV=development concurrently \"gulp build-bg\" \"gulp spawn-watch\" \"node --max_old_space_size=16192 --trace-warnings  ./node_modules/webpack-dev-server/bin/webpack-dev-server --host=0.0.0.0 --config ./webpack.config\"",
    "dev-firefox": "cross-env DEV_ENV=firefox NODE_ENV=development concurrently \"gulp build-bg\" \"gulp spawn-watch\" \"node --max_old_space_size=16192 --trace-warnings  ./node_modules/webpack-dev-server/bin/webpack-dev-server --config ./webpack.config\"",
    "build-bg": "cross-env BUNDLE_ENTRY=bg node --trace-warnings ./node_modules/webpack/bin/webpack --config webpack.config.js --profile --colors",
    "build-app": "cross-env BUNDLE_ENTRY=app node --trace-warnings ./node_modules/webpack/bin/webpack --config webpack.config.js --profile --colors",
    "build-all": "concurrently \"npm run build-app && gulp dist\" \"npm run build-bg\" \"gulp clean\"",
    "build-chrome": "cross-env NODE_ENV=production DEV_ENV=chrome npm run build-all",
    "build-firefox": "cross-env NODE_ENV=production DEV_ENV=firefox npm run build-all",
    "build-sources": "cross-env NODE_ENV=production SKIP_MINIFY=true npm run build-firefox"
  },
  "homepage": "https://chrome.google.com/webstore/detail/tab-master-5000-tab-swiss/mippmhcfjhliihkkdobllhpdnmmciaim",
  "keywords": [
    "firefox",
    "chrome",
    "extension",
    "tabs",
    "productivity",
    "tool",
    "utility"
  ],
  "author": "jaszhix <jaszhix@gmail.com>",
  "license": "MIT",
