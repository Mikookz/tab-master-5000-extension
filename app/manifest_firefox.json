    "open_in_tab": false,
    "page": "options.html"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "page_action": {
    "browser_style": true,
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "__MSG_appName__"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "unlimitedStorage",
    "idle",
    "management",
    "bookmarks",
    "history",
    "downloads"
  ],
  "applications": {
    "gecko": {
      "id": "tm5k@tabmaster.org"
    }
  }
