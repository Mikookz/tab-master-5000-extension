  "enableContextMenu": {
    "message": "Enable context menu"
  },
  "enableContextMenuTip": {
    "message": "This option toggles the right-click context menu on and off. If you disable it, some tab control features will not be accessible."
  },
  "enableDraggableTabReordering": {
    "message": "Enable draggable tab re-ordering"
  },
  "enableDraggableTabReorderingTip": {
    "message": "This option allows you to re-order tabs by dragging a tile, or table row, with your cursor."
  },
  "singleNewTab": {
    "message": "Allow only one New Tab per window"
  },
  "singleNewTabTip": {
    "message": "Enabling this option enforces the closing of all New Tabs except the one that is currently focused. This is useful on older computers."
  },
  "closeOnActivate": {
    "message": "Close New Tab on tab activation"
  },
  "closeOnActivateTip": {
    "message": "Choose whether or not TM5K is closed when clicking a tab tile or row."
  },
  "allTabs": {
    "message": "Show tabs across all windows"
  },
  "allTabsTip": {
    "message": "Controls whether or not tabs across all windows are shown in the tabs grid/table views."
  },
  "trackMostUsed": {
    "message": "Track most used tabs"
  },
  "trackMostUsedTip": {
    "message": "Keeps record of how many times a tab is activated, which enables an optional sorting mode."
  },
  "autoDiscard": {
    "message": "Enable automatic discarding of tabs from memory"
  },
  "autoDiscardTip": {
    "message": "This option allows TM5K to automatically clear tabs from memory after they have been inactive for a set amount of time. Clearing tabs from memory will not close the tabs, but Chrome will have to reload them upon activation."
  },
  "autoDiscardClearTime": {
    "message": "Clear tabs from memory if inactive for"
  },
