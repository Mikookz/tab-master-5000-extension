  "animations": {
    "message": "Enable animations"
  },
  "animationsTip": {
    "message": "This option toggles tab action animations as well as the blur effects. Disabling this is useful on lower end computers with limited hardware acceleration."
  },
  "duplicate": {
    "message": "Enable pulsing duplicate tabs"
  },
  "duplicateTip": {
    "message": "This option will make all duplicates tabs pulsate except the first tab. This makes it easier to see how many duplicate tabs you have open."
  },
  "screenshot": {
    "message": "Enable tab screenshots"
  },
  "screenshotTip": {
    "message": "Enabling this feature adds a screen shot of a tab in the tab tile's background once its been clicked. After a screenshot is active, it is stored in Chrome until the page is active again."
  },
  "screenshotDiskUsage": {
    "message": "Screenshot disk usage"
  },
  "faviconsDiskUsage": {
    "message": "Favicon disk usage"
  },
  "screenshotDiskUsageTip": {
    "message": "If you notice performance issues, clearing the screenshot cache occassionally can help. Screenshots older than three days will start being purged once the cache reaches 50MB."
  },
  "screenshotBg": {
    "message": "Enable screenshots in the background on hover"
  },
  "screenshotBgTip": {
    "message": "This setting enables full-size tab screenshots to fill the background of the New Tab page, while you are hovering over a tab with a screenshot. Screenshots are blurred and blended into the background."
  },
  "tooltip": {
    "message": "Enable tooltips"
  },
  "tooltipTip": {
    "message": "Toggles the tooltip you are reading now."
  },
  "alerts": {
    "message": "Enable alerts"
  },
  "alertsTip": {
    "message": "Toggles the notifications appearing in the bottom-right corner of the screen."
  },
  "errorTelemetry": {
    "message": "Enable anonymous error reporting"
  },
  "errorTelemetryTip": {
    "message": "Keeping this enabled helps improve TM5K. When an error is thrown in the code, your browser, OS, IP, and TM5K settings data (excluding the blacklist) are sent to Sentry along with a stack trace, so the developers can pinpoint the error and patch it for the next release."
  },
  "screenshotBgOpacity": {
    "message": "Set background image opacity"
  },
  "screenshotBgOpacityTip": {
    "message": "Controls the strength of the opacity of background screenshots and wallpaper."
  },
  "screenshotBgBlur": {
    "message": "Set background blur strength"
  },
  "screenshotBgBlurTip": {
    "message": "Controls the strength of the blur of background screenshots, wallpaper, and modal backdrops."
  },
  "tabSizeHeight": {
    "message": "Set tile size"
  },
  "tabSizeHeightTip": {
    "message": "Controls the size of the tiles."
  },
  "tablePadding": {
    "message": "Set table row padding"
  },
  "tablePaddingTip": {
    "message": "Controls how much space is around the text and icons on table rows."
  },
  "resetSearchOnClick": {
    "message": "Reset search on search result click"
  },
  "resetSearchOnClickTip": {
    "message": "Resets the search so all items are shown after a search result is clicked."
  },
  "sessionsSync": {
    "message": "Enable session synchronization"
  },
  "sessionsSyncTip": {
    "message": "Enabling session synchronization allows you to keep a saved session persistently up to date with the current Chrome window. Synchronization occurs at a max interval of fifteen seconds."
  },
  "actions": {
    "message": "Enable undoing of tab actions"
  },
  "actionsTip": {
    "message": "This option allows you to undo a tab action by pressing CTRL+Z, or using the right-click context menu on a tab tile while in the tabs view."
  },
  "keyboardShortcuts": {
    "message": "Enable keyboard shortcuts"
  },
  "blacklist": {
    "message": "Enable website blacklist"
  },
  "blacklistTip": {
    "message": "Enter a space separated list of domains, and they will be automatically closed under any circumstance. This is useful for blocking websites which may inhibit productivity, or you simply don't like."
  },
  "blacklistPlaceholder": {
    "message": "Enter a space separated list of domains..."
